{
  "name": "cudami-editor",
  "version": "5.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint src/**/*.{js,jsx}",
    "start": "webpack --config webpack.dev.js",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@fisker/prettier-config",
  "dependencies": {
    "@aeaton/prosemirror-footnotes": "^0.1.0",
    "@aeaton/react-prosemirror": "^0.22.1",
    "@babel/runtime": "^7.16.5",
    "classnames": "^2.3.1",
    "filesize": "^8.0.6",
    "i18next": "^21.6.0",
    "lodash": "^4.17.21",
    "object.fromentries": "^2.0.5",
    "prosemirror-commands": "^1.1.12",
    "prosemirror-dropcursor": "^1.4.0",
    "prosemirror-gapcursor": "^1.2.0",
    "prosemirror-history": "^1.2.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-model": "^1.15.0",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-schema-list": "^1.1.6",
    "prosemirror-tables": "^1.1.1",
    "pubsub-js": "^1.9.4",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-date-picker": "^8.3.6",
    "react-dom": "^17.0.2",
    "react-file-drop": "^3.1.2",
    "react-i18next": "^11.15.1",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.32.5",
    "react-movable": "^3.0.2",
    "react-paginate": "^8.1.0",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.10.0",
    "scheduler": "^0.20.2",
    "use-context-selector": "^1.3.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@fisker/prettier-config": "^5.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.10",
    "eslint-plugin-react": "^7.27.1",
    "import-sort-style-module": "^6.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-loader": "^8.3.0",
    "typescript": "^4.4.4",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "module"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
